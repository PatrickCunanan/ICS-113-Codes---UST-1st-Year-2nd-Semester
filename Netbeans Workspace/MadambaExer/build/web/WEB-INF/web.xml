<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>serv1</servlet-name>
        <servlet-class>newpackage.serv1</servlet-class>
        <init-param>
            <param-name>driver1</param-name>
            <param-value>config</param-value>
        </init-param>
    </servlet>
    <context-param>
        <param-name>driver</param-name>
        <param-value>context</param-value>
    </context-param>
    <servlet>
        <servlet-name>serv2</servlet-name>
        <servlet-class>newpackage.serv2</servlet-class>
    </servlet>
       <context-param>
            <param-name>Name</param-name>
            <param-value>Juan</param-value>
            </context-param>
        <context-param>
            <param-name>Age</param-name>
            <param-value>age</param-value>
            </context-param>
            <context-param>
            <param-name>Address</param-name>
            <param-value>address</param-value>
        </context-param>
    <servlet-mapping>
        <servlet-name>serv1</servlet-name>
        <url-pattern>/serv1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>serv2</servlet-name>
        <url-pattern>/serv2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
